{
  "name": "dot-dotty",
  "version": "0.6.0",
  "description": "Lazy dot-syntax object access via proxies",
  "main": "dist/index.js",
  "scripts": {
    "test": "ava",
    "build": "rollup src/index.mjs --file dist/index.js --format umd -p terser --name DotDotty",
    "docs": "jsdoc-md -s src/index.mjs -m README.md"
  },
  "keywords": [
    "dot",
    "dot-syntax",
    "lazy",
    "Proxy"
  ],
  "author": "Ketchetwahmeegwun T. Southall",
  "license": "MPL-2.0",
  "devDependencies": {
    "ava": "^3.15.0",
    "jsdoc-md": "^7.0.0",
    "rollup": "^2.23.0",
    "rollup-plugin-terser": "^6.1.0"
  },
  "repository": "github:kettek/dot-dotty",
  "ava": {
    "nodeArguments": [
      "--experimental-modules",
      "--no-warnings"
    ]
  }
}
